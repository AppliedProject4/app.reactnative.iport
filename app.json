{
    "expo": {
        "name": "iPort",
        "slug": "iport",
        "version": "0.0.1",
        "orientation": "portrait",
        "icon": "./src/assets/icon.png",
        "platforms": [
            "android",
            "ios"
        ],
        "splash": {
            "image": "./src/assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "bundleIdentifier": "com.appliedproject.iport"
        },
        "android": {
            "package": "com.appliedproject.iport"
        }
    }
}
